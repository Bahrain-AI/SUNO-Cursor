{
  "name": "ai-music-video-generator",
  "version": "1.0.0",
  "description": "AI Music Video Generator using SUNO and Gemini 3",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
